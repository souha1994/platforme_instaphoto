								<a class="dropdown-item" href="{{path ('image_new')}}">Ajouter Photos</a>
							</div>
						</li>
						{% else %}
						<li class="nav-item">
								<a class="nav-link" href="{{path ('image_index')}}">Images</a>
							</li>
						{% endif %}
					</ul>

						
					</ul>
				</div>
			</div>
		</nav>
	</body>
